<?xml version="1.0"?>

<jetscape>
  
  <nEvents> 1000 </nEvents>
  
  <outputFilename>test_out</outputFilename>
  <JetScapeWriterAscii> off </JetScapeWriterAscii>
  <JetScapeWriterFinalStateHadronsAscii> on </JetScapeWriterFinalStateHadronsAscii> 
  <!-- Hard Process -->
  <Hard>
    <PythiaGun>
      <pTHatMin>0</pTHatMin>
      <pTHatMax>-1</pTHatMax>
      <eCM>5020</eCM>
      <FSR_on>1</FSR_on>
      <LinesToRead>
        HardQCD:all = off
	PromptPhoton:all = off
	Top:gg2ttbar = on
	Top:qqbar2ttbar = on
	24:onMode = off
	24:onIfAny = 13	14
	6:m0 = 172.5
        Tune:preferLHAPDF = 2
	Tune:pp 14
	Tune:ee 7
	MultipartonInteractions:pT0Ref=2.4024
	MultipartonInteractions:ecmPow=0.25208
	ParticleDecays:allowPhotonRadiation = on
	ParticleDecays:limitTau0 = on
	ParticleDecays:tau0Max = 10
      </LinesToRead>
    </PythiaGun>
  </Hard>
  
  <!--Eloss Modules -->
  <Eloss>
    <Matter>
      <Q0> 1.0 </Q0>
      <in_vac> 1 </in_vac>
      <qhat0> 0.0 </qhat0>
      <vir_factor> 0.0015 </vir_factor>
      <recoil_on> 0 </recoil_on>
      <broadening_on> 0 </broadening_on>
      <brick_med> 0 </brick_med>
    </Matter>
  </Eloss>
  
  <!-- Jet Hadronization Module -->
  <JetHadronization>
    <name>colorless</name>
    <eCMforHadronization>2510</eCMforHadronization>
  </JetHadronization>
  
</jetscape>
