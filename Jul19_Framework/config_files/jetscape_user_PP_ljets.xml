<?xml version="1.0"?>

<jetscape>
  
  <nEvents> 1000 </nEvents>
  
  <outputFilename>test_out</outputFilename>
  <!--JetScapeWriterAscii> on </JetScapeWriterAscii-->
  <JetScapeWriterFinalStateHadronsAscii> on </JetScapeWriterFinalStateHadronsAscii> 
  <!-- Hard Process -->
  <Hard>
    <PythiaGun>
      <pTHatMin>0</pTHatMin>
      <pTHatMax>-1</pTHatMax>
      <eCM>5020</eCM>
      <FSR_on>1</FSR_on>
      <LinesToRead>
	Top:gg2ttbar = on
	Top:qqbar2ttbar = on
	HardQCD:all = off
	PromptPhoton:all = off
	<!-- Dilepton -->
	24:onMode = off
	24:onIfAny = 11 12
	24:onIfAny = 13 14
	24:onIfAny = 15 16	
	<!-- l+jets -->
	<!-- 24:onMode = off -->
	<!-- 24:onPosIfAny = 11 13 15 --> 
	<!-- 24:onNegIfAny = 1 2 3 4 5 -->
	<!----------------------> 
	6:m0 = 172.5
	Tune:preferLHAPDF = 2
	Tune:pp 14
	Tune:ee 7
	MultipartonInteractions:pT0Ref=2.4024
	MultipartonInteractions:ecmPow=0.25208
	ParticleDecays:allowPhotonRadiation = on
	ParticleDecays:limitTau0 = on
	ParticleDecays:tau0Max = 10
      </LinesToRead>
    </PythiaGun>
  </Hard>
  
  <!--Eloss Modules -->
  <Eloss>
    <Matter>
      <Q0> 1.0 </Q0>
      <in_vac> 1 </in_vac>
      <qhat0> 0.0 </qhat0>
      <vir_factor> 0.0015 </vir_factor>
      <recoil_on> 0 </recoil_on>
      <broadening_on> 0 </broadening_on>
      <brick_med> 0 </brick_med>
    </Matter>
  </Eloss>
  
  <!-- Jet Hadronization Module -->
  <JetHadronization>
    <name>colorless</name>
    <eCMforHadronization>2510</eCMforHadronization>
  </JetHadronization>
  
</jetscape>
